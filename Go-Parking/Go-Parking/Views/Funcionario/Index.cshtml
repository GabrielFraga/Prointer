@model IEnumerable<Go_Parking.Models.ApplicationUser>

@{
    ViewBag.Title = "Funcionários";
}

<div class="boxPagina">
    <h2>Funcionários</h2>

    <p>
        @Html.ActionLink("Cadastrar funcionário", "Cadastrar")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
           
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                   
                </td>
                <td>
                    @Html.ActionLink(".", "Editar", new { id = item.Id }, htmlAttributes: new { @class = "glyphicon glyphicon-pencil btn" })
                    @Html.ActionLink(".", "Detalhes", new { id = item.Id }, htmlAttributes: new { @class = "glyphicon glyphicon-search btn" })
                    @Html.ActionLink(".", "Deletar", new { id = item.Id }, htmlAttributes: new { @class = "glyphicon glyphicon-trash btn btn" })
                </td>
            </tr>
        }

    </table>
</div>
